<div class="container-fluid">
    <br>
    <div class="row justify-content-md-center">
        <div class="col-md-10">
            <ul class="nav nav-tabs">
                <li class="nav-item">
                    <a class="nav-link active" data-toggle="tab" href="#download">Download Data</a>
                </li>
                <% unless @manage_projects.empty? %>
                    <li class="nav-item">
                        <a class="nav-link" data-toggle="tab" href="#manage">Manage Access</a>
                    </li>
                <% end %>
            </ul>
        </div>
        <!-- Tab panes -->
        <div class="tab-content">
            <div class="tab-pane container active" id="download"><%= render 'project/partials/download' %></div>
            <% unless @manage_projects.empty? %>
                <div class="tab-pane container fade" id="manage"><%= render 'project/partials/manage' %></div>
            <% end %>
        </div>
    </div>
</div>
