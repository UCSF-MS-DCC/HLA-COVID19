<div class="container-fluid" >
    <div class="row" id="home-page-card">
        <div class="col-md-3 ml-auto" id="session-links">
            <% if !current_user %>
                <%= link_to "Sign up", new_registration_path(@user), :class => "session-link" %>
                <%= link_to "Log in", new_session_path(@user), :class => "session-link" %>
            <% else %>
                <%= link_to "Edit password", edit_user_password_path, :class => "session-link"%>
                <%= link_to "Logout", destroy_user_session_path, :method => 'delete', :class => "session-link"%>
            <% end %>
        </div>
        <h1 class="col-lg-12 text-center" id="home-page-card-header">HLA-COVID19<br>Project</h1>
    </div>
</div>